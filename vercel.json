{
    "version": 2,
    "rewrites": [
        {
            "source": "/((?!assets/).*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/*.js": {
            "runtime": "nodejs20.x",
            "memory": 1024,
            "maxDuration": 60
        }
    }
}